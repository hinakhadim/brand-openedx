{
  "name": "@hinakhadim/test-brandedx",
  "version": "1.0.0",
  "description": "The default branding and SASS theme package containing for Open edX applications. This package is designed to be copied and customized.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hinakhadim/brand-openedx.git"
  },
  "author": "Open edX Community",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/hinakhadim/brand-openedx/issues"
  },
  "homepage": "https://github.com/hinakhadim/brand-openedx#readme",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "tagFormat": "testing-${version}",
    "branches": [
      "master",
      "hina/v2"
    ]
  },
  "scripts": {
  
    "build-design-tokens": "build-design-tokens --source ./tokens/src --build-dir ./paragon/css --source-tokens-only",
    "build-scss": "build-scss --corePath ./paragon/core.scss --themesPath ./paragon/css/themes --defaultThemeVariants light",
    "build": "make build"
  },
  "devDependencies": {
    "@edx/paragon": "21.0.0-alpha.40"
  }
}
